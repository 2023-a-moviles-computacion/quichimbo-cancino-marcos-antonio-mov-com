<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditarDistribucion">

    <TextView
        android:id="@+id/tv_EditarDistro"
        android:layout_width="326dp"
        android:layout_height="58dp"
        android:text="Editar distro"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.364"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/tv_nombre_distro_editar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Nombre de la distro"
        app:layout_constraintEnd_toEndOf="@+id/tv_EditarDistro"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_EditarDistro"
        app:layout_constraintTop_toBottomOf="@+id/tv_EditarDistro" />

    <TextView
        android:id="@+id/tv_arquitecturaDistro_editar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Arquitectura"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.089"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.259" />

    <TextView
        android:id="@+id/tv_version_distro_editar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Version"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.085"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.403" />

    <TextView
        android:id="@+id/tv_licencia_distro_editar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Licencia"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.544" />

    <TextView
        android:id="@+id/tv_fecha_lanz_distro_editar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fecha de lanzamiento"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.109"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.683" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtIn_nombreDistro_editar"
        android:layout_width="333dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:hint="Ejemplo: Windows 10"
        app:layout_constraintBottom_toTopOf="@+id/tv_arquitecturaDistro_editar"
        app:layout_constraintEnd_toEndOf="@+id/tv_nombre_distro_editar"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_nombre_distro_editar"
        app:layout_constraintTop_toBottomOf="@+id/tv_nombre_distro_editar"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtIn_arquitecturaDistro_editar"
        android:layout_width="333dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:hint="X86"
        app:layout_constraintBottom_toTopOf="@+id/tv_version_distro_editar"
        app:layout_constraintEnd_toEndOf="@+id/tv_arquitecturaDistro_editar"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_arquitecturaDistro_editar"
        app:layout_constraintTop_toBottomOf="@+id/tv_arquitecturaDistro_editar"
        app:layout_constraintVertical_bias="0.03" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtIn_versionDistro_editar"
        android:layout_width="333dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:hint="2"
        app:layout_constraintBottom_toTopOf="@+id/tv_licencia_distro_editar"
        app:layout_constraintEnd_toEndOf="@+id/tv_version_distro_editar"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_version_distro_editar"
        app:layout_constraintTop_toBottomOf="@+id/tv_version_distro_editar"
        app:layout_constraintVertical_bias="0.071" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtIn_licenciaDistro_editar"
        android:layout_width="333dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:hint="Open GL"
        app:layout_constraintBottom_toTopOf="@+id/tv_fecha_lanz_distro_editar"
        app:layout_constraintEnd_toEndOf="@+id/tv_licencia_distro_editar"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_licencia_distro_editar"
        app:layout_constraintTop_toBottomOf="@+id/tv_licencia_distro_editar"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtIn_fechaLanzDistro_editar"
        android:layout_width="333dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:hint="2022"
        app:layout_constraintEnd_toEndOf="@+id/tv_fecha_lanz_distro_editar"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_fecha_lanz_distro_editar"
        app:layout_constraintTop_toBottomOf="@+id/tv_fecha_lanz_distro_editar" />

    <Button
        android:id="@+id/btn_editar_distribucion"
        android:layout_width="201dp"
        android:layout_height="43dp"
        android:backgroundTint="#023E8A"
        android:text="Guardar Cambios"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtIn_fechaLanzDistro_editar"
        app:layout_constraintVertical_bias="0.212" />

    <Button
        android:id="@+id/btn_cancelar_distribucion_editar"
        android:layout_width="115dp"
        android:layout_height="39dp"
        android:layout_marginTop="60dp"
        android:backgroundTint="#0077B6"
        android:text="Cancelar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtIn_fechaLanzDistro_editar"
        app:layout_constraintVertical_bias="0.307" />

</androidx.constraintlayout.widget.ConstraintLayout>